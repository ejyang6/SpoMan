<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration 
PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 1) 타입 별칭 지정 -->
	<typeAliases>
		<typeAlias type="spo.tis.domain.UserVO" alias="User"/>
		<typeAlias type="spo.tis.domain.ClubVO" alias="Club2"/>
		<typeAlias type="spo.tis.domain.MemberVO" alias="Member"/>
		<typeAlias type="spo.tis.domain.CPictureVO" alias="CPicture"/>
		<typeAlias type="spo.tis.domain.CBoardVO" alias="CBoard"/>
		
		<typeAlias type="spo.tis.domain.matchaddVO" alias="matchadd"/>
		<typeAlias type="spo.tis.domain.TeamVO" alias="Team"/>
		<typeAlias type="spo.tis.domain.McVO" alias="Mc"/>
		<typeAlias type="spo.tis.domain.MtVO" alias="Mt"/>
		<typeAlias type="spo.tis.domain.PagingVO" alias="Paging"/>
		<typeAlias type="spo.tis.domain.Paging2VO" alias="Paging2"/>
		<typeAlias type="spo.tis.domain.Paging3VO" alias="Paging3"/>
	</typeAliases>

	<!-- 2) mapper 파일 등록 -->
	<mappers>
		<mapper resource="spring/mapper/UserMapper.xml"/>
		<mapper resource="spring/mapper/ClubMapper.xml"/>
		<mapper resource="spring/mapper/MemberMapper.xml"/>
		<mapper resource="spring/mapper/CPictureMapper.xml"/>
		<mapper resource="spring/mapper/CBoardMapper.xml"/>
		
		<mapper resource="spring/mapper/MatchMapper.xml"/>
		<mapper resource="spring/mapper/TeamMapper.xml"/>
		<mapper resource="spring/mapper/MyDataMapper.xml"/>
	</mappers>
</configuration>